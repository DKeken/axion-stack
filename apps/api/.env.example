# Environment
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/nestjs_api?schema=public"

# JWT Secrets (use strong secrets in production)
JWT_ACCESS_SECRET="your-super-secret-access-key-min-32-characters-long"
JWT_REFRESH_SECRET="your-super-secret-refresh-key-min-32-characters-long"
JWT_ACCESS_EXPIRES_IN="15m"
JWT_REFRESH_EXPIRES_IN="7d"

# Security
CORS_ORIGIN="http://localhost:3000,http://localhost:3001"
RATE_LIMIT_TTL=60
RATE_LIMIT_LIMIT=100

# HTTP Proxy (optional)
HTTP_PROXY_ENABLED=false
HTTP_PROXY_BASE_URL="https://api.example.com"
HTTP_PROXY_TIMEOUT=30000