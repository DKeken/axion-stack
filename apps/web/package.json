{
  "name": "@repo/web",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf .next",
    "dev": "npm run clean && paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && next dev --turbo",
    "build": "npm run clean && npm run wagmi:generate && paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && next build",
    "build:prod": "npm run clean && (npm run wagmi:generate || echo 'wagmi:generate skipped') && paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && next build",
    "start": "next start",
    "check-types": "tsc --noEmit --project tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "machine-translate": "inlang machine translate --project project.inlang",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" --ignore-path .prettierignore",
    "wagmi:generate": "wagmi generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@repo/contracts": "workspace:*",
    "@shikijs/transformers": "^3.9.1",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.1",
    "@tanstack/react-table": "^8.21.3",
    "@ts-rest/react-query": "^3.52.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.542.0",
    "media-chrome": "^4.13.0",
    "motion": "^12.23.12",
    "next": "^15.3.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.0.0",
    "react-fast-marquee": "^1.6.5",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.1.2",
    "remark-gfm": "^4.0.1",
    "shiki": "^3.9.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "2.0.1",
    "tailwindcss": "4.1.11",
    "tw-animate-css": "^1.3.8",
    "use-sound": "^5.0.0",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@inlang/cli": "^3.0.12",
    "@inlang/paraglide-js": "^2.2.0",
    "@tailwindcss/postcss": "^4.0.0-beta.8",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.3.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2"
  }
}
